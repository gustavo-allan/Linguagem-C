#include<stdlib.h>
#include<stdio.h>

main()

{
	int n1,n2,n3;
	
	printf("digite o primeiro numero: ");
	scanf("%d",& n1);
	
	printf("digite o segundo numero: ");
	scanf("%d",& n2);
	
	printf("digite o terceiro numero: ");
	scanf("%d",& n3);
	
	if (n1 > n2 && n1 >n3)
	{
         printf("o maior numero digitado foi : %.2d ",n1);
	}
	if (n2 > n1 && n2 > n3)
	{
		printf("o maior numero digitado foi : %.2d ",n2);
	}
	if (n3 > n1 && n3 > n2)
	{
		printf("o maior numero digitado foi : %.2d ",n3);
	}
	printf("\n");
	// numeros menores
	if(n1 < n2 && n1 < n3) 
	{
		printf("o menor numero digitado foi : %.2d ",n1);
	}
	if(n2 < n1 && n2 < n3)
    {
	    printf("o menor numero digitado foi : %.2d ",n2);
    }
	if(n3 < n1 && n3 < n2)
	{
		printf("o menor numero digitado foi : %.2d ",n3);
	}
	
}
